<h1>Moment Edit</h1>
<form method="post" action="/moments/<%= @moment.id %>">
<input id="hidden" type="hidden" name="_method" value="patch">
<input type="text" name="name" value="<%= @moment.name %>" placeholder="<%= @moment.name %>">

<input class="btn btn-primary" type="submit" name="edit" value="Edit">
</form>
